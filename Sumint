package Day9;
interface Sum
{
	int max=100;
	void show();
	static void display()
	{
	System.out.println("Displaying");	
	}
}
class myclass implements Sum
{
	public void show(){
		System.out.println("Showing");
	}
}
public class Suminter {

	public static void main(String[] args) {
		Sum.display();
		myclass obj=new myclass ();
		obj.show();

	}

}
