package Day9;
class aanimal
{
	String color="Red";
}
class Bird
{
	String color="Purple";
}
class Dog extends Bird{
	String color="White";
	void display()
	{
		System.out.println(color);
		System.out.println(super.color);
	}
}

public class Super {
	public static void main(String[]args) {
		Dog obj=new Dog();
		obj.display();
	}

}
